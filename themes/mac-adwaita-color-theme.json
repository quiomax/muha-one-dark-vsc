    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "variable.other.constant",
        "boolean",
        "null"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.character",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "support.type"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    }
